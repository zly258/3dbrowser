{
  "name": "@zhangly1403/3dbrowser",
  "private": false,
  "version": "1.5.3",
  "description": "A high-performance 3D model browser based on Three.js, supporting IFC, 3D Tiles, GLB, and more.",
  "keywords": [
    "threejs",
    "bim",
    "ifc",
    "3dtiles",
    "3d-viewer",
    "browser"
  ],
  "license": "CC-BY-NC-4.0",
  "author": "zhangly1403@163.com",
  "type": "module",
  "main": "./dist/3dbrowser.umd.js",
  "module": "./dist/3dbrowser.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/3dbrowser.es.js",
      "require": "./dist/3dbrowser.umd.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && npx tsc -p tsconfig.lib.json --skipLibCheck && xcopy /E /I /Y dist example\\3dbrowser-lib",
    "build:example": "cd example && npm install && npm run build",
    "preview": "vite preview"
  },
  "dependencies": {
    "3d-tiles-renderer": "0.3.31",
    "occt-import-js": "^0.0.23",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "three": "^0.181.2",
    "web-ifc": "^0.0.74"
  },
  "devDependencies": {
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "terser": "^5.46.0",
    "typescript": "~5.8.2",
    "vite": "^6.4.1"
  }
}
